class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vecter<int> ret;
        for(int i=0; i<nums.size();i++){
            for(int j=i; j<nums.size();j++){
            if (nums[i]+nums[i+1]==target){
            ret.pushback(i);
            ret.pushback(i+1);
        }
        }
        }
    }
};
