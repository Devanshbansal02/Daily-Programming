class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int n = nums.size(), k;
        set<int> s;
        for(int i=0; i<n;i++){
            s.insert(nums[i]);
        }
        k=s.size();
        auto it = s.begin();
        for(int i=0; i<k ;i++){
            nums[i]=*it;
            it++;
        }
        return k;
    }
};
